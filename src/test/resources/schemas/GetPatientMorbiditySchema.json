    "properties": {
      "fileId": { "type": "string" },
      "fileName": { "type": "string" },
      "uploadDate": { "type": "string" },
      "morbidConditions": {
        "type": "object",
        "properties": {
          "T3": { "type": "string" },
          "T4": { "type": "string" },
          "TSH": { "type": "string" },
          "HbA1c": { "type": "string" },
          "Average Glucose": { "type": "string" },
          "Plasma Glucose": { "type": "string" },
          "Fasting Glucose": { "type": "string" }
